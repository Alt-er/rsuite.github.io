{
  "presets": [
    "stage-0",
    "react",
    [
      "env",
      {
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "ie >= 9"
          ]
        }
      }
    ]
  ],
  "plugins": [
    [
      "import",
      {
        "libraryName": "rsuite",
        "camel2DashComponentName": false
      }
    ],
    "transform-class-properties",
    "transform-runtime",
    "add-module-exports"
  ]
}