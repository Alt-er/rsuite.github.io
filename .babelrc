{
  "presets": [
    "stage-0",
    "react",
    [
      "env",
      {
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "ie >= 9"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "transform-class-properties",
    "transform-runtime",
    "add-module-exports"
  ],
  "env": {
    "production": {
      "presets": [
        "react-optimize"
      ]
    }
  }
}